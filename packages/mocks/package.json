{
  "name": "@kubev2v/mocks",
  "version": "0.0.1",
  "private": true,
  "repository": "git@github.com:kubev2v/forklift-console-plugin.git",
  "license": "Apache-2.0",
  "main": "./src/index.ts",
  "scripts": {
    "postinstall": "msw init ./generated --save",
    "clean": "rimraf ./dist ./coverage tsconfig.tsbuildinfo",
    "clean:all": "npm run clean -- ./node_modules ./.rollup.cache",
    "build": "npm run clean && npm run compile && npm run copy",
    "compile": "tsc --build --verbose",
    "copy": "copyfiles -u 1 ./generated/** ./dist",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "msw": "^1.2.1"
  },
  "devDependencies": {
    "ts-node": "^10.9.1"
  },
  "msw": {
    "workerDirectory": "generated"
  }
}