{
  "name": "@kubev2v/mocks",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "url": "git@github.com:kubev2v/forklift-console-plugin.git",
    "directory": "packages/mocks"
  },

  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./consoleHandlers": {
      "types": "./dist/consoleHandlers/index.d.ts",
      "default": "./dist/consoleHandlers/index.js"
    }
  },

  "scripts": {
    "clean": "rimraf ./dist ./coverage tsconfig.tsbuildinfo",
    "clean:all": "npm run clean -- ./node_modules ./.rollup.cache",
    "build": "npm run clean && npm run compile",
    "compile": "tsc --build --verbose",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@kubev2v/legacy": "*",
    "msw": "^1.2.1"
  },
  "devDependencies": {
    "typescript": "^4.7.4"
  }
}
