{
  "name": "forklift-console-plugin",
  "version": "0.0.1",
  "private": true,
  "repository": "git@github.com:yaacov/forklift-console-plugin.git",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && NODE_ENV=production yarn ts-node node_modules/.bin/webpack",
    "build:dev": "yarn clean && yarn ts-node node_modules/.bin/webpack",
    "start": "yarn ts-node node_modules/.bin/webpack serve",
    "start:console": "./scripts/start-console.sh",
    "start:console:oauth": "./scripts/start-console-with-oauth.sh",
    "i18n": "i18next \"src/**/*.{js,jsx,ts,tsx}\" [-oc] -c i18next-parser.config.mjs",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
    "lint": "eslint ./src && stylelint \"src/**/*.css\" --allow-empty-input",
    "lint:fix": "eslint ./src --fix && stylelint \"src/**/*.css\" --allow-empty-input --fix",
    "test": "jest",
    "test:i18n": "./scripts/test-i18n.sh",
    "test:coverage": "jest --coverage",
    "test:updateSnapshot": "jest --updateSnapshot"
  },
  "devDependencies": {
    "@openshift-console/dynamic-plugin-sdk": "0.0.12",
    "@openshift-console/dynamic-plugin-sdk-webpack": "0.0.7",
    "@patternfly/react-core": "4.175.4",
    "@patternfly/react-table": "^4.93.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.37",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "i18next-parser": "^6.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1",
    "prettier-stylelint": "^0.4.2",
    "react": "17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.11",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "license": "Apache-2.0",
  "consolePlugin": {
    "name": "forklift-console-plugin",
    "version": "0.0.1",
    "displayName": "OpenShift Console Plugin For Forklift",
    "description": "Forklift is a suite of migration tools that facilitate the migration of VM workloads to KubeVirt.",
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  }
}
