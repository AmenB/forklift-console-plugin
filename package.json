{
  "name": "forklift-console-plugin",
  "version": "0.0.1",
  "private": true,
  "repository": "git@github.com:kubev2v/forklift-console-plugin.git",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && NODE_ENV=production yarn webpack",
    "start": "yarn webpack serve",
    "start:console": "source ./scripts/configure/openshift.sh && ./scripts/start-console.sh",
    "start:console:oauth": "source ./scripts/configure/openshift-auth.sh && ./scripts/start-console.sh",
    "i18n": "i18next \"src/**/*.{js,jsx,ts,tsx}\" [-oc] -c i18next-parser.config.mjs",
    "lint": "eslint . && stylelint \"src/**/*.css\" --allow-empty-input",
    "lint:fix": "eslint . --fix && stylelint \"src/**/*.css\" --allow-empty-input --fix",
    "test": "jest",
    "test:i18n": "./scripts/test-i18n.sh",
    "test:coverage": "jest --coverage",
    "test:updateSnapshot": "jest --updateSnapshot",
    "helm:lint": "helm lint deployment/forklift-console-plugin/",
    "helm:index": "helm repo index ./tmp/ --url https://kubev2v.github.io/forklift-console-plugin",
    "helm:build": "mkdir -p tmp && helm package -d tmp deployment/forklift-console-plugin/ && yarn helm:index"
  },
  "devDependencies": {
    "@migtools/lib-ui": "^8.4.1",
    "@openshift-console/dynamic-plugin-sdk": "0.0.17",
    "@openshift-console/dynamic-plugin-sdk-webpack": "0.0.8",
    "@openshift/dynamic-plugin-sdk": "~1.0.0",
    "@openshift/dynamic-plugin-sdk-webpack": "~1.0.0",
    "@patternfly/react-core": "4.221.3",
    "@patternfly/react-table": "4.90.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.12",
    "@types/file-saver": "^2.0.3",
    "@types/immutable": "^3.8.7",
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.1",
    "@types/netmask": "^1.0.30",
    "@types/node": "^18.0.0",
    "@types/q": "^1.5.4",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/simple-oauth2": "^4.1.0",
    "@types/victory": "^33.1.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "axios": "^0.21.2",
    "classnames": "2.x",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dayjs": "^1.11.5",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "file-saver": "1.3.x",
    "i18next-parser": "^6.5.0",
    "immutable": "3.x",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "netmask": "^2.0.2",
    "prettier": "^2.7.1",
    "prettier-stylelint": "^0.4.2",
    "q": "^1.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
